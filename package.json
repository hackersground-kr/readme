    "@testing-library/user-event": "^13.5.0",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.13.0",
    "react-scripts": "5.0.1",
